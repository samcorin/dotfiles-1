[alias]
  ci = commit -v
  up = !sh -c 'git remote update && git config --get-regexp svn-remote > /dev/null && git svn fetch'
  co = checkout
  br = branch
  st = status
  di = diff
  df = diff --no-prefix
  wt = whatchanged --stat
  svf = svn fetch
  svr = svn rebase
  svd = svn dcommit
  new = whatchanged HEAD@{1}..HEAD --reverse --stat

